{
  "name": "wasm-backporter",
  "version": "0.0.1",
  "description": "Backport post-MVP WASM features to older instructions on client-side",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "types": "dist/lib/index.d.ts",
  "scripts": {
    "start": "ts-node lib/index.ts",
    "build": "sh -c ./scripts/build.sh",
    "eslint": "eslint lib/**/*.ts test/**/*.ts",
    "prepublishOnly": "",
    "tsc": "npx tsc --noEmit",
    "test": "node --expose-gc --experimental-wasm-simd --wasm-simd-post-mvp ./node_modules/jest/bin/jest --coverage --logHeapUsage",
    "testSimd": "node --experimental-wasm-simd --wasm-simd-post-mvp ./node_modules/jest/bin/jest simdCompareFloat"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Daninet/wasm-backporter.git"
  },
  "keywords": [
    "wasm",
    "webassembly",
    "backport",
    "babel",
    "post-mvp",
    "simd",
    "bulk",
    "memory",
    "transform",
    "compatibility",
    "compat",
    "replace",
    "transpile",
    "recompile",
    "fast",
    "instructions",
    "instruction",
    "backward",
    "compatible"
  ],
  "bugs": {
    "url": "https://github.com/Daninet/wasm-backporter/issues"
  },
  "homepage": "https://github.com/Daninet/wasm-backporter#readme",
  "author": "Dani Bir√≥ <danibiro93@gmail.com> (https://danibiro.com)",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.3",
    "jest": "^26.1.0",
    "rollup": "^2.23.0",
    "ts-jest": "^26.1.3",
    "ts-loader": "^8.0.1",
    "ts-node": "^8.10.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.7",
    "wabt": "1.0.19-nightly.20200821"
  }
}
